{
	"name": "front-end",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "dotenv -e .env.development vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"db:push": "drizzle-kit push",
		"db:generate-dev": "dotenv -e .env.development -- drizzle-kit generate",
		"db:migrate-dev": "dotenv -e .env.development -- drizzle-kit migrate",
		"db:generate-prod": "dotenv -e .env.production -- drizzle-kit generate",
		"db:migrate-prod": "dotenv -e .env.production -- drizzle-kit migrate",
		"db:studio-dev": "dotenv -e .env.development -- drizzle-kit studio",
		"db:studio-prod": "dotenv -e .env.production -- drizzle-kit studio",
		"seed:dev": "dotenv -e .env.development -- tsx src/lib/server/db/seed/admin.ts",
		"seed:prod": "dotenv -e .env.production -- tsx src/lib/server/db/seed/admin.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.22.0",
		"@inlang/paraglide-js": "^2.3.2",
		"@internationalized/date": "^3.9.0",
		"@lucide/svelte": "^0.544.0",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@types/bcrypt": "^6.0.0",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/node": "^20",
		"@types/pg": "^8.15.5",
		"autoprefixer": "^10.4.21",
		"bcrypt": "^6.0.0",
		"bits-ui": "^2.11.0",
		"clsx": "^2.1.1",
		"depcheck": "^1.4.7",
		"dotenv": "^17.2.2",
		"dotenv-cli": "^10.0.0",
		"drizzle-kit": "^0.30.2",
		"drizzle-orm": "^0.40.0",
		"eslint": "^9.22.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"formsnap": "^2.0.1",
		"globals": "^16.0.0",
		"postcss": "^8.5.6",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"sveltekit-superforms": "^2.27.1",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^3.1.1",
		"tailwindcss": "^4.0.0",
		"tsx": "^4.20.6",
		"tw-animate-css": "^1.3.8",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4",
		"zod": "^4.1.11"
	},
	"dependencies": {
		"@neondatabase/serverless": "^0.10.4",
		"@node-rs/argon2": "^2.0.2",
		"cloudinary": "^2.7.0",
		"jsonwebtoken": "^9.0.2",
		"mode-watcher": "^1.1.0",
		"pg": "^8.16.3",
		"svelte-sonner": "^1.0.5"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		],
		"ignoredBuiltDependencies": [
			"@tailwindcss/oxide",
			"bcrypt"
		]
	}
}